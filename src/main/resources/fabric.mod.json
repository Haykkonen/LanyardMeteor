{
  "schemaVersion": 1,
  "id": "lanyard",
  "version": "${version}",
  "name": "Lanyard",
  "description": "A Meteor Client addon for network analysis, development, and utilities.",
  "authors": [
    "Haykkonen"
  ],
  "contact": {
    "homepage": "https://github.com/haykkonen/lanyard",
    "sources": "https://github.com/haykkonen/lanyard"
  },
  "icon": "assets/lanyard/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.haykkonen.lanyard.Lanyard"
    ]
  },
  "mixins": [
    "lanyard.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
